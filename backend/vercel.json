{
    "builds": [
        {
            "src": "/server/app.py",
            "use": "@vercel/python"
        },
        {
            "src": "/lib/**",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/teams",
            "dest": "/server/app.py"
        },
        {
            "src": "/api/fantasy",
            "dest": "/server/app.py"
        },
        {
            "src": "/api/predictions",
            "dest": "/server/app.py"
        }
    ]
}